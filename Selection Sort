#include <stdio.h>

int main(){
    int size=7;
    int arr[7]={1,3,4,9,6,10,5};
    for(int i=0;i<size-1;i++){
        int minidx=i;
        for(int j=i+1;j<size;j++){
            if(arr[j]<arr[minidx]) minidx=j;
        }
        int temp=arr[i];
        arr[i]=arr[minidx];
        arr[minidx]=temp;
    }

    for(int i=0;i<size;i++){
        printf("%d ",arr[i]);
    }
    return 0;
}
