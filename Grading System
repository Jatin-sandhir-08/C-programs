#include <stdio.h>

/* Criteria:-
A = Marks>=90
B = Marks>=80
C = Marks>=70
D = Marks>=60
E = Marks>=50
F = Marks>=40
Fail = Marks<40*/

int main(){
    int s1,s2,s3,s4,s5,total;
    printf("Enter Marks in first subject:");
    scanf("%d",&s1);
    printf("Enter Marks in second subject:");
    scanf("%d",&s2);
    printf("Enter Marks in third subject:");
    scanf("%d",&s3);
    printf("Enter Marks in fourth subject:");
    scanf("%d",&s4);
    printf("Enter Marks in fifth subject:");
    scanf("%d",&s5);
    total = (s1+s2+s3+s4+s5)/5;
    if(total>=90){
        printf("A grade\n");
    } else if(total>=80){
        printf("B grade\n");
    } else if(total>=70){
        printf("C grade\n");
    } else if(total>=60){
        printf("D grade\n");
    } else if(total>=50){
        printf("E grade\n");
    } else if(total>=40){
        printf("F grade\n");
    } else{
        printf("Fail\n");
    }
    return 0;
}
