#include <stdio.h>

int main(){
    int size=10;
    int a[10]={0,3,5,7,9,13,15,19,25,34};
    int target,ans=-1;
    printf("Enter Target:");
    scanf("%d",&target);
    int st=0,end=size-1;        
    while(st<=end){
        int mid=(st+end)/2;
        if(a[mid]==target){
            ans=mid;
            break;
        } else if(a[mid]>target){
            end=mid-1;
        } else if(a[mid]<target){
            st = mid+1;
        }
    }
    if(ans==-1){
        printf("Target not Found!\n");
    } else{
        printf("Target is on index %d\n",ans);
    }
    return 0;
}
