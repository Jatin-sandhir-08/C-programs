#include <stdio.h>

int main(){
    int a[] = {1,2,4,6,5,9};
    int size = sizeof(a)/sizeof(a[0]);
    int target;
    printf("Enter Target:");
    scanf("%d",&target);
    for(int i=0;i<size-1;i++){
        for(int j=i+1;j<size;j++){
            if(a[i]+a[j]==target){
                printf("Index = %d,%d\n",i,j);
                
            }
        }
    }
    return 0;
}
