#include <stdio.h>
#include <limits.h>

int main(){
    int a[]={19,28,35,88,49,50,51};
    int large = INT_MIN;
    int small = INT_MAX;
    int size = sizeof(a)/sizeof(a[0]);
    for(int i=0;i<size;i++){
        large = large>a[i]?large:a[i];
    } 
    for(int i=0;i<size;i++){
        small = small<a[i]?small:a[i];
    }
    printf("Smallest no. is %d\n",small);
    printf("Largest no. is %d\n",large);
    return 0;
}
