#include <stdio.h>
#include <stdbool.h>

int main(){
    int size=7;
    int arr[7]={1,3,4,9,5,10,6};
    for(int i=0;i<size-1;i++){
        bool flag=true;
        for(int j=0;j<size-1;j++){
            if(arr[j]>arr[j+1]){
                int temp = arr[j];
                arr[j]=arr[j+1];
                arr[j+1]=temp;
                flag=false;
            }
        }
        if(flag==true) break;
    }
    for(int i=0;i<size;i++){
        printf("%d ",arr[i]);
    }
    return 0;
}
